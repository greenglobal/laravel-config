{
    "name": "ggphp/config",
    "description": "laravel config",
    "keywords": [
        "ggphp",
        "configuration"
    ],
    "homepage": "https://github.com/greenglobal/laravel-config",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "TuanNT",
            "email": "tuannt@greenglobal.vn",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "kreait/laravel-firebase": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.3",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "GGPHP\\Config\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GGPHP\\Config\\Tests\\": "tests"
        }
    },
    "scripts": {
        "lint": [
            "vendor/bin/phpcs --standard=PSR12 ./src ./config ./tests"
        ],
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "GGPHP\\Config\\Providers\\ConfigServiceProvider"
            ],
            "aliases": {}
        }
    }
}
